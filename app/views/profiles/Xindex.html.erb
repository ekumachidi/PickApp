<div class="page-header">
  <%= link_to new_profile_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Profile
  <% end %>
  <h1>Listing profiles</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>Fullname</th>
        <th>Address</th>
        <th>Phone</th>
        <th>latitude</th>
        <th>longitude</th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @profiles) do |profile| %>
          <td><%= Profile.name %></td>
          <td><%= Profile.address %></td>
          <td><%= Profile.phone %></td>
          <td><%= Profile.latitude %></td>
          <td><%= Profile.longitude %></td>
            <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
