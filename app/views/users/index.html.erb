
  <h1>Users</h1>
<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>name</th>
        <th>role</th>
        <th>location</th>
         <th>email</th>
        <th>Phone</th>
        <th>Change Role</th>
      </tr>
    </thead>

    <tbody>
      <%@normal.each do |user| %>
      <tr>

        <td><%= user.profile.name rescue "User" %></td>
        <td><%= user.role.name %></td>
        <td><%= user.profile.address rescue "unknown"%></td>
        <td><%= user.email %></td>
        <td><%= user.profile.phone rescue "address" %></td>
        <td><%= link_to edit_user_path(user), class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Change role </td>
    </tr>
    <%end%>
    <%end%>
    </tbody>

